<!DOCTYPE html>
<html dir="ltr" lang="en-US">
<head runat="server">
	<meta charset="UTF-8" />
	<meta name="description" content="CliqFlip is a social platform that connects people
	on sociality, personality, and commonalitys" />
	<meta name="keywords" content="cliqflip, sociality, personality, commonality" />
	<link rel="shortcut icon" href="@Url.Content("/Content/img/favicon.png")" type="image/png" />
	<title>@ViewBag.Title</title>
	<!-- Website Styles -->
	<link rel="stylesheet" type="text/css" media="all" href="@Url.Content("/Content/styles/skins/default-light.css")" />
	<link rel="stylesheet" type="text/css" media="all" href="@Url.Content("/Content/styles/anythingslider.css")" />
	<link rel="stylesheet" type="text/css" media="all" href="@Url.Content("/Content/styles/skitter.styles.css")" />
	<link rel="stylesheet" type="text/css" media="all" href="@Url.Content("/Content/js/colorbox/colorbox.css")" />
	<!-- IE HTML5 Hack -->
	<!--[if lt IE 9]>
			<script src="//html5shim.googlecode.com/svn/trunk/html5.js"> </script>
		<![endif]-->
	<!-- Google Font -->
	<link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css' />
	<link href='http://fonts.googleapis.com/css?family=PT+Sans+Narrow' rel='stylesheet' type='text/css' />
    <style type="text/css">
        #header
        {
            background:#1A1E25 url(/Content/img/anythingSlider/bgSlider.jpg) no-repeat top center;
            position:relative;
            padding-top: 20px;
            height: 65px;
            border-bottom: 5px solid royalBlue;
            position: static;
        }
        .content
        {
            width: 100%;
        }
    </style>
	@RenderSection("styles",false)
</head>
<body>
	<div id="main-container">
		<!-- HEADER START -->
		<div id="header" class="wrapper">
			<div class="inner">
				<div class="logo">
					<a href="#" class="logo_img">CliqFlip</a>
				</div>
				<div class="motto">Discover <span>More</span>
				</div>
				<div class="back">
				</div>
			</div>
		</div>
		<!-- HEADER END -->
		@RenderBody()
		<!-- FOOTER START -->
		@*<div id="footer_box" class="full-width">
			<div class="wrapper">
                <div class="inner">
                asd
                </div>
			</div>
		</div>*@
		<!-- FOOTER END -->
		<!-- COPYRIGHT START -->
		<div id="copyright_box">
			<div class="wrapper">
				<div class="inner">
					<div class="divider">
					</div>
					<em>&copy; @DateTime.Now.Year <a href="index.html">CliqFlip</a> - All rights reserved.</em>
					<div class="divider">
					</div>
				</div>
			</div>
		</div>
		<!-- COPYRIGHT END -->
	</div>
	<!-- end #main-container -->
	<!-- Scripts -->
	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/colorbox/jquery.colorbox-min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.quicksand.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.skitter.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.animate-colors-min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery-easing.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.anythingslider.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.anythingslider.fx.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.cycle.all.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jflickrfeed.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery-ui-1.8.16.custom.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/twitterjs-2.0.0.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.unobtrusive-ajax.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.validate.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/jquery.validate.unobtrusive.min.js")"> </script>
	<script type="text/javascript" src="@Url.Content("/Content/js/core.js")"> </script>
	@RenderSection("scripts",false)
</body>
</html>
