<div class="row-fluid">
    <div class="span3">
        <img src="{{ProfileImageUrl}}" alt="{{Username}}"/>
    </div>
    <div class="span9">
        <div class="row-fluid">
            <div class="span12">
                <span class="label label-info">
                    {{Username}}
                </span>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <h6>Posted in {{Interest}}</h6>
            </div>
        </div>
    </div>
</div>
<div class="row-fluid">
    <div class="span12">
        <img class="feed-image" src="{{MediumImage}}" alt="{{Description}}"/>
    </div>
</div>