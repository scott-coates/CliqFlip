<div class="interest-feed-item box-grey">
	<div style="float:left;width:210px;">
		<h4>{{model.Username}}</h4>
		<a value="{{medium.MediumId}}" title="{{model.Username}}" href="{{model.UserPageUrl}}">
			<img class="profile-image" src="{{model.ImageUrl}}" alt="{{model.Username}}"/>
		</a>
	</div>

	<div style="float:right;width:210px;">
		<h4 style="text-align:right;">{{model.Interest}}</h4>
		<a style="display:inline-block;float:right" value="{{medium.MediumId}}" title="{{model.Description}}" href="{{model.FullImage}}" medium="{{model.MediumType}}">
			<img class="profile-image" src="{{model.MediumImage}}" alt="{{model.Description}}"/>
		</a>
	</div>

	<div style="width:107px;margin:0 260px 0 210px;text-align:center;padding-top:50px;background-image: url('/Content/img/CliqFlip-Arrow.png');background-repeat:no-repeat;background-size:75%;background-position:0 70px ">
		<h3>Posted</h3>
		<h3 style="margin-top:30px;">In</h3>
	</div>

	<div class="clear"></div>
	<p>{{model.Description}}</p>
</div>